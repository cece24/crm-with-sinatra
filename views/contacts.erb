<h1>Contact List</h1>
<main>
  <% @contacts.each do |contact| %>
    <article>
      <ul>
        <li>First Name: <%= contact.first_name %></li>
        <li>Last Name: <%= contact.last_name %></li>
        <li>Email: <%= contact.email %></li>
        <li>Note: <%= contact.note %></li>
      </ul>
    </article>
  <% end %>
</main>
<a href="/">Home</a>
